{
  "name": "browserSignIn",
  "version": "1.0.0",
  "runtimeVersion": "1.0.0",
  "assetBundlePatterns": [
    "**/*"
  ],
  "icon": "./app_logo.png",
  "android": {
    "package": "com.browsersignin"
  },
  "ios": {
    "bundleIdentifier": "com.browsersignin"
  },
  "plugins": [
    [
      "expo-splash-screen",
      {
        "backgroundColor": "#ffffff",
        "image": "./app_logo.png",
        "dark": {
          "image": "./app_logo.png",
          "backgroundColor": "#000000"
        },
        "imageWidth": 200
      }
    ],
    [
      "expo-build-properties",
      {
        "android": {
          "kotlinVersion": "2.0.21"
        }
      }
    ]
  ]
}
