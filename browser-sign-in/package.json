{
  "name": "browser-sign-in",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "test": "jest",
    "lint": "eslint .",
    "doctor": "react-native doctor",
    "pod-reinstall": "rm -rf ./ios/Pods && rm -f ./ios/Podfile.lock && npx --yes pod-install --non-interactive",
    "build-ios": "react-native build-ios",
    "build-android": "react-native build-android",
    "run-ios": "react-native run-ios",
    "run-android": "react-native run-android",
    "clean": "react-native clean && rm -rf ./node_modules && rm -rf ./.expo && yarn",
    "clean-ios": "rm -rf ./ios/build && rm -rf ./ios/Pods",
    "clean-android": "cd android && ./gradlew clean && cd .. && rm -rf ./android/build && rm -rf ./android/.gradle && rm -rf ./android/.idea"
  },
  "dependencies": {
    "@okta/okta-react-native": "~2.17.0",
    "@react-native-community/cli": "^15.0.0",
    "@react-native/metro-config": "^0.77.2",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "expo": "^52.0.46",
    "expo-build-properties": "~0.13.3",
    "expo-splash-screen": "~0.29.13",
    "expo-status-bar": "~2.0.0",
    "expo-updates": "~0.26.9",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.77.2",
    "react-native-dotenv": "^3.4.9",
    "react-native-gesture-handler": "~2.25.0",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "5.4.1",
    "react-native-screens": "~4.11.1",
    "semver": "^7.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@types/react": "~18.3.12",
    "typescript": "^5.3.3"
  },
  "jest": {
    "preset": "react-native",
    "automock": false,
    "transformIgnorePatterns": [
      "node_modules/(?!@okta|react-native|@react-native)"
    ],
    "setupFiles": [
      "./setupJest.js"
    ]
  }
}
